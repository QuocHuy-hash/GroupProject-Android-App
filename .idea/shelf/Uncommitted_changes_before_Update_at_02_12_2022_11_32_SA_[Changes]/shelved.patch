+    }
+
+    public ArrayList<Uri> getUrisArrAnh() {
+        return urisArrAnh;
+    }
+
+    public void setUrisArrAnh(ArrayList<Uri> urisArrAnh) {
+        this.urisArrAnh = urisArrAnh;
+    }
+
+    public String getStrTitle() {
+        return strTitle;
+    }
+
+    public void setStrTitle(String strTitle) {
+        this.strTitle = strTitle;
+    }
+
+    public String getStrDescripsion() {
+        return strDescripsion;
+    }
+
+    public void setStrDescripsion(String strDescripsion) {
+        this.strDescripsion = strDescripsion;
+    }
+
+    public String getStrFee() {
+        return strFee;
+    }
+
+    public void setStrFee(String strFee) {
+        this.strFee = strFee;
+    }
+
+    public String getStrTime() {
+        return strTime;
+    }
+
+    public void setStrTime(String strTime) {
+        this.strTime = strTime;
+    }
+
+    public int getSoluongAnh() {
+        return soluongAnh;
+    }
+
+    public void setSoluongAnh(int soluongAnh) {
+        this.soluongAnh = soluongAnh;
+    }
+
+    public boolean isFavorite() {
+        return favorite;
+    }
+
+    public void setFavorite(boolean favorite) {
+        this.favorite = favorite;
+    }
+}
