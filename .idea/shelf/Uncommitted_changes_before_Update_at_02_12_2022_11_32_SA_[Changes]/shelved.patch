ew.ViewHolder {\r\n            TextView tvTenDanhMuc;\r\n            LinearLayout layout_item;\r\n        public ViewHolder(@NonNull View itemView) {\r\n            super(itemView);\r\n            layout_item = itemView.findViewById(R.id.layout_item);\r\n            tvTenDanhMuc = itemView.findViewById(R.id.tvTenDanhMuc);\r\n        }\r\n    }\r\n}\r\n
